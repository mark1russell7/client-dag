{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/client-dag",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "sideEffects": false,
  "devDependencies": {
    "typescript": "^5.9.3",
    "@mark1russell7/cue": "github:mark1russell7/cue#main",
    "vitest": "^3.0.0",
    "@mark1russell7/test": "github:mark1russell7/test#main"
  },
  "description": "Generic DAG algorithms for dependency management"
}
